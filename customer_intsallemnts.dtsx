              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select onp.installment_id as INSTALLMENT_ID,
op.legacy_id as ORDER_ITEM_ID,
onp.no_installments as TOTAL_INSTALLMENTS,
onp.installment_paid as NO_OF_INSTALLMENTS_PAID, 
onp.ord_prod_id as ord_prod_id_int, 
onp.acct_id,onp.amount,
a.bb_acct_id as ACCOUNT_ID 
,ins.description as INSTALLMENT_STATUS
 from onetime_payment  onp
 inner join ordered_product op on op.ord_prod_id = onp.ord_prod_id 
 inner join acct_order_item aoi on aoi.ord_prod_id = op.ord_prod_id
 inner join acct_order ao on ao.order_id = aoi.order_id
 inner join account a on a.acct_id = ao.acct_id
 inner join contact c on c.contact_id = a.contact_id
 inner join business_unit bu on bu.bu_id = a.bu_id
 inner join installment_status ins on ins.id = onp.installment_status_id
where bu.external_system='DAZN'
and c.migrated_user ='T'</property>
