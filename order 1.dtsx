              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select a.bb_acct_id  as account_id,
ao.legacy_id  as order_id,
ao.external_order_id   as ext_order_id,
ao.promise_date  as order_date,
op.legacy_id  as order_item_id,
op.prod_id  as prod_id, -- (Need to confirm with phani)
acs.description  as order_status,
op.srv_start_date  as valid_from,
op.validity_end_date as valid_to,
op.rec_indicator  as opt_renew,
-- och.consent_startdate  as opt_renew_date,
op.cancellation_date  as cancel_date,
op.comments as comment,
op.is_Renewable ,
-- op2.legacy_id as parent_order_item_id,
ao.pay_meth_id  as payment_method_id_int
,op.linked_op_id as linked_order_item_id,
op.applied_prod_rate_id  as product_rate_id,
op.reason_code_id  as reason_id,
aoi.order_item_id as order_item_id_int,
'' as payment_method_id
from account a
inner join contact c on c.contact_id = a.contact_id
inner join acct_order ao on ao.acct_id=a.acct_id
inner join acct_order_item aoi on aoi.order_id = ao.order_id
inner join ordered_product op on op.ord_prod_id = aoi.ord_prod_id
 left join product p on op.prod_id=p.prod_id
-- left join ordered_product op2 on op2.ord_prod_id=op.parent_ord_prod_id
inner join account_status acs on op.acct_status_id=acs.acct_stat_id
-- left join order_attributes oa on oa.order_id =ao.order_id
-- left join op_consent_history och on och.op_id=op.ord_prod_id
inner join business_unit bu on a.bu_id = bu.bu_id
where bu.external_system like 'DAZN'
and c.migrated_user = 'T'
and op.legacy_id is not NULL</property>
