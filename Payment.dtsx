              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select
  pa.legacy_id  as payment_id
, pa.payment_id as payment_id_int
,pa.payment_amt
,pa.order_id as order_id_int
,ao.legacy_id as order_id
,pa.create_date
,pa.received_date
,pa.posting_status_id
,pa.payment_type
,pa.is_refund
,gs.trans_id
,gs.response
,pa.payment_method_id as payment_method_id_int
,'' as payment_method_id
,gs.error_code
,pa.parent_payment_id
,pa.currency_code
,pa.retries
,pa.last_retry_date
,ps.description as posting_status
,cct.c_c_trans_id,
gs.id,
cct.responce_code ,
a.bb_acct_id as Account_id
from  payment pa
left join payment_method pm on pa.payment_method_id=pm.pay_method_id
left join cc_transactions cct on pa.payment_id = cct.payment_id
left join gateway_response gs on cct.c_c_trans_id = gs.cc_trans_id
left join   acct_order ao  on pa.order_id = ao.order_id
inner join  account a on a.acct_id = pa.acct_id
inner join contact c on a.contact_id=c.contact_id
left join payment_type pt on pm.type_id=pt.id
left join posting_status ps on pa.posting_status_id=ps.id
inner join business_unit bu on bu.bu_id=a.bu_id
where bu.external_system='DAZN'
and c.migrated_user= 'T'</property>
